[
    {
        "description": "Retrieve all products with price greater than $50",
        "query": {
            "collection": "Products",
            "filter": { "price": { "$gt": 50 } }
        }
    },
    {
        "description": "Find all customers from New York",
        "query": {
            "collection": "Customers",
            "filter": { "city": "New York" }
        }
    },
    {
        "description": "Get all sales made in the last 30 days",
        "query": {
            "collection": "Sales",
            "filter": {
                "date": {
                    "$gte": { "$date": "2025-03-24T00:00:00Z" }
                }
            }
        }
    }
]
