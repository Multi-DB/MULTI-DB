[
    {
        "description": "Get Computer Science Students",
        "query": {
            "action": "get_entity",
            "entity": "Students",
            "filters": {"Major": "Computer Science"}
        }
    },
    {
        "description": "Get Hackathon Names and Project Titles",
        "query": {
            "action": "get_entity",
            "entity": "HackathonParticipations",
            "fields": ["HackathonName", "ProjectTitle", "ActivityID"]
        }
    },
    {
        "description": "Get all Student Club Memberships",
        "query": {
            "action": "get_entity",
            "entity": "StudentClubs"
        }
    },
    {
        "description": "Find Courses taken by Student John Smith (ID 1001)",
        "query": {
            "action": "get_related",
            "start_entity": "Students",
            "start_filters": {"StudentID": 1001},
            "relations": [
                {"relation": "REFERENCES", "direction": "in", "target_entity": "Enrollments"},
                {"relation": "REFERENCES", "direction": "out", "target_entity": "Courses"}
            ],
            "final_fields": {
                "Students": ["FirstName", "LastName"],
                "Enrollments": ["Semester", "Year", "Grade"],
                "Courses": ["CourseName", "CourseCode"]
            }
        }
    },
    {
        "description": "Find Hackathons participated in by Biology students",
        "query": {
            "action": "get_related",
            "start_entity": "Students",
            "start_filters": {"Major": "Biology"},
            "relations": [
                {"relation": "REFERENCES", "direction": "in", "target_entity": "HackathonParticipations"}
            ],
            "final_fields": {
                "Students": ["FirstName", "LastName", "Major"],
                "HackathonParticipations": ["HackathonName", "ProjectTitle", "AwardsWon"]
            }
        }
    },
    {
        "description": "Find Clubs joined by Student Sophia Lee (ID 1004)",
        "query": {
            "action": "get_related",
            "start_entity": "Students",
            "start_filters": {"StudentID": 1004},
            "relations": [
                {"relation": "REFERENCES", "direction": "in", "target_entity": "StudentClubs"}
            ],
            "final_fields": {
                "Students": ["FirstName", "LastName"],
                "StudentClubs": ["ClubName", "Role", "JoinDate"]
            }
        }
    },
    {
        "description": "Find Sports activities of Students enrolled in 'CS101'",
        "query": {
            "action": "get_related",
            "start_entity": "Courses",
            "start_filters": {"CourseCode": "CS101"},
            "relations": [
                {"relation": "REFERENCES", "direction": "in", "target_entity": "Enrollments"},
                {"relation": "REFERENCES", "direction": "out", "target_entity": "Students"},
                {"relation": "REFERENCES", "direction": "in", "target_entity": "SportsParticipations"}
            ],
            "final_fields": {
                "Courses": ["CourseName"],
                "Students": ["FirstName", "LastName", "Major"],
                "SportsParticipations": ["SportName", "TeamName", "Achievements"]
            }
        }
    }
]
